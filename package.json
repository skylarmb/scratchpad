{
  "name": "@skylarmb/scratchpad",
  "version": "0.0.0",
  "description": "code scratchpad",
  "type": "module",
  "author": "Skylar Brown",
  "license": "ISC",
  "main": "src/main.ts",
  "scripts": {
    "down": "docker compose down",
    "start:build": "docker compose up --build --detach",
    "start:logs": "docker compose logs -f scratchpad --no-log-prefix",
    "start": "run-s start:build start:logs",
    "watch": "nodemon src/index.ts",
    "storybook": "dotenv -- start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "dotenv -e .env.test -- $_ jest --watchAll",
    "test-debug": "dotenv -e .env.test -v NODE_OPTIONS='--inspect-brk=0.0.0.0:9229' -v NODE_INSPECT_RESUME_ON_START=1 jest --runInBand --watchAll --detectOpenHandles"
  },
  "prettier": "./prettierrc.json",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/debug": "^4.1.7",
    "@types/faker": "^5.5.9",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "axios": "^1.3.4",
    "babel-loader": "^9.1.2",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "faker": "5.5.3",
    "jest": "^29.4.3",
    "node-dev": "^8.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "plaid-threads": "^14.5.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "require-relative": "^0.8.7",
    "signal-exit": "^3.0.7",
    "storybook-css-modules": "^1.0.8",
    "supports-color": "^9.3.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
